{
  "build": {
    "beforeBuildCommand": "trunk build",
    "beforeDevCommand": "trunk serve",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "TauriLeptosTemplate",
    "version": "0.0.0"
  },
  "plugins": {
    "shell": {
      "open": true
    },
    "updater": {
      "endpoints": []
    }
  },
  "tauri": {
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "net.curiana",
      "targets": "all",
      "updater": {
        "active": false
      }
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 460,
        "resizable": true,
        "title": "TauriLeptos",
        "width": 300
      }
    ]
  }
}
